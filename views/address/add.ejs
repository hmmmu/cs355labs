<%- include("../includes/header.ejs", {title:"Add New Address"}); %>
    <h1>Add New Address</h1>
    <form action="./insert" method="get">
        Street Address: <input id="street" name="street" type="text" />
        <br />
        Zip Code: <input id="zip_code" name="zip_code" type="text" />
        <!-- Initialize the multiselect plugin: -->
        <script type="text/javascript">
            $(document).ready(function() {
                $('#address_id').multiselect();
            });
        </script>
        <br />
        <input type="submit" />
    </form>
<%- include("../includes/footer.ejs"); %>
